@model DBSmartAPIManager.DAL.Entities.ProjectFile

@{
    ViewData["Title"] = "Delete Project File";
}

<h2>Delete Project File</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Project File</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            File Name
        </dt>
        <dd class="col-sm-10">
            @Model.FileName
        </dd>
        <dt class="col-sm-2">
            File Way
        </dt>
        <dd class="col-sm-10">
            @Model.FileWay
        </dd>

    </dl>

    <form asp-action="DeleteConfirmed" method="post">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger"/> |
        <a asp-action="Index">Back to List</a>
    </form> 
</div>
